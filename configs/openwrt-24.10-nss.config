## Use "make defconfig" to expand this to a full .config
## Target
CONFIG_TARGET_ipq806x_generic_DEVICE_netgear_r7800=y
CONFIG_TARGET_ipq806x_generic=y
CONFIG_TARGET_ipq806x=y

CONFIG_EXPERIMENTAL=y

## Use ath10k instead ath10k-ct
CONFIG_PACKAGE_ath10k-firmware-qca9984=y
# CONFIG_PACKAGE_ath10k-firmware-qca9984-ct is not set
CONFIG_PACKAGE_kmod-ath10k=y
CONFIG_ATH10K_LEDS=y
CONFIG_ATH10K_THERMAL=y
# CONFIG_PACKAGE_kmod-ath10k-ct is not set

CONFIG_FEED_sqm_scripts_nss=n

# NSS Drivers
CONFIG_PACKAGE_kmod-nss-ifb=y
CONFIG_PACKAGE_kmod-qca-mcs=y
CONFIG_PACKAGE_kmod-qca-nss-cfi-cryptoapi=y
CONFIG_PACKAGE_kmod-qca-nss-crypto=y
CONFIG_PACKAGE_kmod-qca-nss-drv=y
CONFIG_PACKAGE_kmod-qca-nss-drv-igs=y
CONFIG_PACKAGE_kmod-qca-nss-drv-l2tpv2=m
CONFIG_PACKAGE_kmod-qca-nss-drv-pppoe=y
CONFIG_PACKAGE_kmod-qca-nss-drv-pptp=m
CONFIG_PACKAGE_kmod-qca-nss-drv-qdisc=m
CONFIG_PACKAGE_kmod-qca-nss-drv-tun6rd=m
CONFIG_PACKAGE_kmod-qca-nss-drv-tunipip6=m
CONFIG_PACKAGE_kmod-qca-nss-gmac=m
CONFIG_PACKAGE_kmod-qca-nss-ecm-standard=y
CONFIG_PACKAGE_MAC80211_NSS_SUPPORT=y
CONFIG_PACKAGE_MAC80211_DEBUGFS=y

## Luci app
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-app-sqm=y
CONFIG_PACKAGE_luci-theme-material=y

## SQM QoS nss
CONFIG_PACKAGE_sqm-scripts-nss=y
